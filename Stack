-- Scripts:

local function NDCBU_fake_script() -- Target.LocalScript 
	local script = Instance.new('LocalScript', Target)

	Target = script.Parent
	
	Target.FocusLost:connect(function()
		for i,v in pairs(game.Players:GetChildren()) do
			if(string.sub(string.lower(v.Name),1,string.len(Target.Text))) == string.lower(Target.Text) then
				Target.Text = v.Name
			end
		end
	end)
end
coroutine.wrap(NDCBU_fake_script)()
local function MVUJN_fake_script() -- KillButton.LocalScript 
	local script = Instance.new('LocalScript', KillButton)

	local button = script.Parent
	local target = script.Parent.Parent.Target
	_G.tog2 = true
	_G.on = 1
	
	function Hitbox(target)
	    local x = game.Players.LocalPlayer.Character.HumanoidRootPart.Position.x--just general position
        local y = game.Players.LocalPlayer.Character.HumanoidRootPart.Position.y
        local z = game.Players.LocalPlayer.Character.HumanoidRootPart.Position.z
	    game:GetService("Workspace")[target.text].Head.Size = Vector3.new(100, 100, 100)
	    game:GetService("Workspace")[target.text].Head.Position = Vector3.new(x,y,z)
        game:GetService("Workspace")[target.text].Head.Transparency = 1
	end
	
	function BringBack()
		wait(2.5)
		for i, v in pairs(game.Workspace.Storage:GetDescendants()) do-- gets the body velocity 
			if v.name == "EnergySphere9" then
				v.BV.Velocity = Vector3.new(0, -2200, 0)--Changes the velocity to make the snowball fire back down
			end
		end
	end
	
	button.MouseButton1Click:Connect(function()
		while _G.tog2 == true do--loop
			wait()
			BringBack()-- calls the function to bring the snowballs down
			Hitbox(Target)--calls the hitbox function
		end
	end)

	button.MouseButton2Click:Connect(function()
		while _G.on == 1 do
	
			wait()
	
			local args = {
				[1] = {
					[1] = "Skill_SpherePunch",
					[2] = Vector3.new(-367,15735000,-10)
				}
			}
	
			game:GetService("ReplicatedStorage").RemoteEvent:FireServer(unpack(args))
			game:GetService("ReplicatedStorage").RemoteEvent:FireServer(unpack(args))
			game:GetService("ReplicatedStorage").RemoteEvent:FireServer(unpack(args))
			game:GetService("ReplicatedStorage").RemoteEvent:FireServer(unpack(args))
			game:GetService("ReplicatedStorage").RemoteEvent:FireServer(unpack(args))
			game:GetService("ReplicatedStorage").RemoteEvent:FireServer(unpack(args))
		end
	end)
	
	
end
coroutine.wrap(MVUJN_fake_script)()
local function OROZR_fake_script() -- DestroyGui.LocalScript 
	local script = Instance.new('LocalScript', DestroyGui)

	Exit = script.Parent
	a = script.Parent.Parent.Parent.Parent
	
	Exit.MouseButton1Click:Connect(function()
		a:Destroy()
	end)
end
coroutine.wrap(OROZR_fake_script)()
local function UUSTWKP_fake_script() -- RejoinButton.LocalScript 
	local script = Instance.new('LocalScript', RejoinButton)

	local button2 = script.Parent
	
	button2.MouseButton1Click:Connect(function()
		game:GetService('TeleportService'):TeleportToPlaceInstance(game.PlaceId, game.JobId, cmdp)
	end)
end
coroutine.wrap(UUSTWKP_fake_script)()
local function HANA_fake_script() -- Mainframe.UiFrame 
	local script = Instance.new('LocalScript', Mainframe)

	local frame = script.Parent
	
	local player = game.Players.LocalPlayer
	
	local userId = player.UserId
	local thumType = Enum.ThumbnailType.AvatarBust
	local thumSize = Enum.ThumbnailSize.Size420x420
	local content = game.Players:GetUserThumbnailAsync(userId, thumType, thumSize)
	
	frame.PlayerImage.Image = content -- change the name of these acording
	frame.PlayerName.Text = player.Name-- this aswell
end
coroutine.wrap(HANA_fake_script)()
